<?xml version="1.0" encoding="ISO-8859-1"?>

<project name="${artifactId}" xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 http://maven.apache.org/xsd/decoration-1.8.0.xsd">
    <bannerLeft>
        <name>${artifactId}</name>
        <src>https://www.spigotmc.org/data/resource_icons/2/2124.jpg</src>
        <href>https://docs.skinsrestorer.net/</href>
    </bannerLeft>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.8</version>
    </skin>

    <body>
        <head>
            <![CDATA[<meta name="theme-color" content="#C970D9">]]>
            <![CDATA[<meta property="og:title" content="SkinsRestorer">]]>
            <![CDATA[<meta property="description" content="Ability to restore/change skins on servers! (Offline and Online Mode). (Bungee/Spigot/Sponge)">]]>
            <![CDATA[<meta property="og:description" content="Ability to restore/change skins on servers! (Offline and Online Mode). (Bungee/Spigot/Sponge)">]]>
            <![CDATA[<meta property="og:image" content="https://www.spigotmc.org/data/resource_icons/2/2124.jpg">]]>
        </head>

        <links>
            <item name="Github" href="https://github.com/SkinsRestorer/SkinsRestorerX"/>
            <item name="Discord" href="https://discord.gg/012gnzKK9EortH0v2"/>
            <item name="SpigotMC" href="https://www.spigotmc.org/resources/2124/"/>
            <item name="Ore" href="https://ore.spongepowered.org/SRTeam/SkinsRestorer"/>
        </links>

        <!--
        <menu name="Overview">
            <item name="About" href="/index.html"/>
            <item name="Config" href="/config.html"/>
            <item name="Formatting" href="/format.html"/>
            <item name="Placeholders" href="/placeholders.html"/>
            <item name="API" href="/api.html"/>
            <item name="FAQ" href="/faq.html"/>
            <item name="Javadocs" href="/apidocs/index.html"/>
        </menu>
        -->

        <menu ref="reports"/>

        <footer><![CDATA[All rights reserved.]]></footer>
    </body>

    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <profile>(production)</profile>
            <skipGenerationDate>true</skipGenerationDate>
        </fluidoSkin>
    </custom>
</project>
